{
  "GitProxy": {
    "EnableCaching": false,
    "CacheExpirationMinutes": 30,
    "MaxRequestSizeMB": 100,
    "TimeoutMinutes": 10,
    "AllowedDomains": [
      "github.com"
    ],
    "UserAgent": "git/2.0.0 (GitProxyCloner/1.0)"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DH.GitProxyCloner": "Debug"
    }
  },
  "AllowedHosts": "*"
}
